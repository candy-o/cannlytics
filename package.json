{
  "name": "cannlytics",
  "version": "0.0.11",
  "description": "Cannlytics includes a LIMS, a console, documentation, a website, and vast amount of tools for cannabis analytics.",
  "keywords": [
    "cannabis",
    "analytics",
    "python",
    "LIMS"
  ],
  "homepage": "https://cannlytics.com",
  "bugs": {
    "url": "https://github.com/cannlytics/cannlytics/issues",
    "email": "contact@cannlytics.com"
  },
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Keegan Leon Skeate",
    "email": "keegan@cannlytics.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cannlytics/cannlytics.git"
  },
  "scripts": {
    "ai:deploy-cannabis-data-daily": "cd ./tools/data/state_data/get_cannabis_data_daily && gcloud functions deploy get_cannabis_data_daily --entry-point get_cannabis_data_daily --runtime python39 --trigger-topic get_cannabis_data_daily --memory 1024MB --timeout 300 --env-vars-file env.yaml && cd ../../../../",
    "console:start": "npm-run-all --parallel console:livereload console:dev console:serve",
    "console:serve": "cd console && webpack serve --env production && cd ../",
    "console:livereload": "set PROJECT=console & python manage.py livereload --ignore-static-dirs",
    "console:dev": "set PROJECT=console & python manage.py runserver",
    "console:collectstatic": "set PROJECT=console & python manage.py collectstatic --noinput",
    "console:build": "webpack --env production --config ./config/console.webpack.config.js",
    "console:container": "npm run console:collectstatic && gcloud builds submit --tag gcr.io/cannlytics/cannlytics-lims",
    "console:cloud": "gcloud run deploy cannlytics-lims --image gcr.io/cannlytics/cannlytics-lims --set-env-vars SETTINGS_NAME=cannlytics_platform_settings --region us-central1 --allow-unauthenticated --platform managed --memory 2Gi --cpu 2 --min-instances 1",
    "console:deploy": "firebase deploy --project cannlytics --only hosting:production",
    "console:publish": "npm run console:build && npm run console:container && npm run console:cloud && npm run console:deploy",
    "docs:install": "docker build -t squidfunk/mkdocs-material docs",
    "docs:build": "mkdocs build",
    "docs:publish": "firebase deploy --project cannlytics --only hosting:docs",
    "docs:serve": "docker run --rm -it -p 8000:8000 -v \"%cd%\":/docs squidfunk/mkdocs-material",
    "website": "TODO: Add all website scripts!"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "@primer/octicons": "^11.0.0",
    "axios": "^0.21.4",
    "bootstrap": "^5.1.3",
    "chart.js": "^2.9.4",
    "chartjs-chart-box-and-violin-plot": "^2.3.0",
    "chartjs-chart-matrix": "^0.1.3",
    "firebase": "^9.6.0",
    "frappe-charts": "^1.5.5",
    "idb": "^5.0.8",
    "particles.js": "^2.0.0",
    "popper.js": "^1.16.1",
    "tether": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@types/autoprefixer": "^10.2.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "dotenv-webpack": "^6.0.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.1",
    "postcss-loader": "^5.3.0",
    "sass": "^1.43.5",
    "sass-loader": "^10.2.0",
    "style-loader": "^1.3.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-node-externals": "^3.0.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
