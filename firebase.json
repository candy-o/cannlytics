{
  "firestore": {
    "indexes": ".firebase/firestore.indexes.json",
    "rules": ".firebase/firestore.rules"
  },
  "storage": {
    "rules": ".firebase/storage.rules"
  },
  "hosting": [
    {
      "target": "docs",
      "public": "docs/build"
    },
    {
      "target": "production",
      "public": "public/console",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/development/**"
      ],
      "rewrites": [
        {
          "source": "!/sitemap",
          "run": {
            "serviceId": "cannlytics-console",
            "region": "us-central1"
          }
        },
        {
          "source": "!/static",
          "run": {
            "serviceId": "cannlytics-console",
            "region": "us-central1"
          }
        },
        {
          "source": "**",
          "run": {
            "serviceId": "cannlytics-console",
            "region": "us-central1"
          }
        }
      ]
    },
    {
      "target": "dev",
      "public": "public/console",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/development/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "run": {
            "serviceId": "cannlytics-dev",
            "region": "us-central1"
          }
        }
      ]
    },
    {
      "site": "cannlytics",
      "public": "public/website",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/development/**"
      ],
      "appAssociation": "AUTO",
      "rewrites": [
        {
          "source": "!/sitemap",
          "run": {
            "serviceId": "cannlytics-website",
            "region": "us-central1"
          }
        },
        {
          "source": "!/static",
          "run": {
            "serviceId": "cannlytics-website",
            "region": "us-central1"
          }
        },
        {
          "source": "**",
          "run": {
            "serviceId": "cannlytics-website",
            "region": "us-central1"
          }
        }
      ]
    }
  ]
}
